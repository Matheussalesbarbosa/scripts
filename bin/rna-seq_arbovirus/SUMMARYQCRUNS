#!/bin/bash
cd $HOME/datasets/rna-seq_arbovirus/qc/qc-r1/ && \
multiqc \
--fullnames \ # manter o nome do arquivo que vai ser analisado
--title ArbovirusBahiaRun1 \ # criar um titulo para o relatorio do multiqc
--interactive \ # utilizar plots iterativos
--export $HOME/datasets/rna-seq_arbovirus/qc/qc-r1/ # exportar plots das analises do multiqc
cd $HOME/datasets/rna-seq_arbovirus/qc/qc-r2/ && \
multiqc \
--fullnames \
--title ArbovirusBahiaRun2 \
--interactive \
--export $HOME/datasets/rna-seq_arbovirus/qc/qc-r2/
cd $HOME/datasets/rna-seq_arbovirus/qc/qc-r3/ && \
multiqc \
--fullnames \
--title ArbovirusBahiaRun3 \
--interactive \
--export $HOME/datasets/rna-seq_arbovirus/qc/qc-r3/
cd $HOME/datasets/rna-seq_arbovirus/qc/qc-r4/ && \
multiqc \
--fullnames \
--title ArbovirusBahiaRun4 \
--interactive \
--export $HOME/datasets/rna-seq_arbovirus/qc/qc-r4/
cd $HOME/datasets/rna-seq_arbovirus/qc/qc-r5/ && \
multiqc \
--fullnames \
--title ArbovirusBahiaRun5 \
--interactive \
--export $HOME/datasets/rna-seq_arbovirus/qc/qc-r5/
